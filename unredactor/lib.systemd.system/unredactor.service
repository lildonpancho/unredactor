[Unit]
Description=Unredactor
After=network.target
After=systemd-user-sessions.service
After=network-online.target

[Service]
User=msoc
Type=simple #forking
ExecStart=/home/msoc/apps/unredactor/runserver.sh
# ExecStop=
TimeoutSec=900
Restart=on-failure
RestartSec=30
KillMode=process
StartLimitInterval=350
StartLimitBurst=10

[Install]
WantedBy=multi-user.target
